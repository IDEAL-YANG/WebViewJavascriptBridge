{
  "name": "LXWebViewJavascriptBridge",
  "version": "6.1.0",
  "summary": "An iOS & OSX bridge for sending messages between Obj-C/Swift and JavaScript in WKWebViews, UIWebViews & WebViews.",
  "homepage": "https://github.com/IDEAL-YANG/WebViewJavascriptBridge",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "IDEAL-YANG": "lxyang6688@163.com"
  },
  "source": {
    "git": "https://github.com/IDEAL-YANG/WebViewJavascriptBridge.git",
    "tag": "v6.1.0"
  },
  "platforms": {
    "ios": "5.0",
    "osx": ""
  },
  "requires_arc": true,
  "ios": {
    "source_files": "WebViewJavascriptBridge/*.{h,m}",
    "private_header_files": "WebViewJavascriptBridge/WebViewJavascriptBridge_JS.h",
    "frameworks": [
      "UIKit",
      "WebKit"
    ]
  },
  "osx": {
    "source_files": "WebViewJavascriptBridge/*.{h,m}",
    "private_header_files": "WebViewJavascriptBridge/WebViewJavascriptBridge_JS.h"
  },
  "frameworks": "WebKit",
  "pod_target_xcconfig": {
    "DEFINES_MODULE": "YES"
  }
}
